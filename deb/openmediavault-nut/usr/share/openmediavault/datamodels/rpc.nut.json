[{
	"type": "rpc",
	"id": "rpc.nut.set",
	"params": {
		"type": "object",
		"properties": {
			"enable": {
			    "type": "boolean",
				"required": true
			},
			"upsname": {
			    "type": "string",
			    "pattern": "^[a-z0-9_-]+$",
				"required": true
			},
			"comment": {
			    "type": "string",
				"required": true
			},
			"mode": {
				"type": "string",
				"enum": [ "netclient", "standalone" ],
				"required": true
			},
			"nutserver": {
				"type": "string",
				"required": true
			},
			"nutserverlogin": {
				"type": "string",
				"required": true
			},
			"nutserverpassword": {
				"type": "string",
				"required": true
			},
			"powervalue": {
				"type": "integer",
				"required": true
			},
			"driverconf": {
			    "type": "string",
				"required": true
			},
			"shutdownmode": {
			    "type": "string",
			    "enum":[ "fsd", "onbatt" ],
				"required": true
			},
			"shutdowntimer": {
			    "type": "integer",
			    "minimum": 1,
				"required": true
			},
			"remotemonitor": {
			    "type": "boolean",
				"required": true
			},
			"remoteuser": {
			    "type": "string",
				"required": true
			},
			"remotepassword": {
			    "type": "string",
				"required": true
			}
		}
	}
}]
